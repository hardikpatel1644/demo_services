{% extends 'SonataAdminBundle:CRUD:base_edit.html.twig' %}

{% block stylesheets %}
    {{ parent() }}
    
    {% stylesheets '@theme_jvectormap_css' %}
        <link rel="stylesheet" href="{{ asset_url }}" type="text/css">
    {% endstylesheets %}
        
    <link rel="stylesheet" href="{{ asset('bundles/ixoiljvectormap/css/jvectormap-field.css') }}" type="text/css">
{% endblock %}

{% block javascripts %}
    {{ parent() }}
    
    {% javascripts '@theme_jvectormap_js' %}
        <script src="{{ asset_url }}" type="text/javascript"></script>
    {% endjavascripts %}
        
    <script src="{{ asset('bundles/ixoiljvectormap/js/jvectormap-field.js') }}" type="text/javascript"></script>
{% endblock %}